@page "/"
@rendermode InteractiveServer

<PageTitle>Home</PageTitle>

<h1>Hello, world!</h1>

<button class="btn btn-primary" @onclick="DoSomething">Click me</button>

@code {
    private async Task DoSomething()
    {
        BlazorApiGeneratedClient.Services.TodoApi test = new("https://localhost:7046/api", new HttpClient());
        var todos = await test.GetAllTodosAsync();
    }
}